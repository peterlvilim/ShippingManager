<Window x:Class="CondensedShippingReport.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edit Routes" Height="505" Width="524">
    <Grid>
        <ListBox x:Name="listRoutes" HorizontalAlignment="Left" Height="438" Margin="14,26,0,0" VerticalAlignment="Top" Width="200"/>
        <Button Click="buttonRemove_Click"  x:Name="buttonRemove" Content="Remove" HorizontalAlignment="Left" Margin="406,26,0,0" VerticalAlignment="Top" Width="100" Height="30"/>
        <Button Click="buttonAdd_Click"  x:Name="buttonAdd" Content="Add" HorizontalAlignment="Left" Margin="406,82,0,0" VerticalAlignment="Top" Width="100" Height="30"/>
        <TextBox x:Name="textName" HorizontalAlignment="Left" Height="30" Margin="225,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <TextBlock x:Name="textTitle" HorizontalAlignment="Left" Margin="14,3,0,0" TextWrapping="Wrap" Text="Current Routes:" VerticalAlignment="Top"/>

    </Grid>
</Window>
